{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "bulma": "0.7.2",
    "classnames": "2.2.6",
    "gatsby": "2.0.77",
    "gatsby-image": "2.0.25",
    "gatsby-plugin-manifest": "2.0.12",
    "gatsby-plugin-offline": "2.0.20",
    "gatsby-plugin-react-helmet": "3.0.5",
    "gatsby-plugin-sass": "2.0.7",
    "gatsby-plugin-sharp": "2.0.17",
    "gatsby-source-filesystem": "2.0.12",
    "gatsby-transformer-sharp": "2.1.9",
    "node-sass": "4.11.0",
    "prop-types": "15.6.2",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-helmet": "5.2.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "format": "npm run format-base -- --write",
    "format-list": "npm run format-base -- --list-different",
    "format-base": "prettier \"src/**/*.{js,json}\" \"*.js\"",
    "lint": "eslint src/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "eslint": "5.11.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.12.4",
    "husky": "1.3.1",
    "prettier": "1.15.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format-list",
      "pre-push": "npm run lint"
    }
  }
}
